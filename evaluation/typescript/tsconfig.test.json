{
  "extends": "./tsconfig",
  "compilerOptions": {
    /* Basic Options */
    "target": "es2017", /* Specify ECMAScript target version. */
    "module": "ESNext", /* Specify module code generation. */
    "moduleResolution": "node",
    "lib": [
      "es2023",
    ],
    "esModuleInterop": true,
    "outDir": "./__test/",
    "types": [
      "@types/node",
    ]
  },
  "exclude": [
    "./src/**/proto/*"
  ],
}